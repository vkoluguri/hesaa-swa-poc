{
  "name": "hesaa-swa-poc",
  "version": "1.0.0",
  "description": "HESAA SWA + SharePoint PoC",
  "private": true,
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "routes": "node tools/generate-swa-routes.js",
    "index": "node tools/build-content-index.js",
    "build:spa": "vite build -c spa/vite.config.ts",
