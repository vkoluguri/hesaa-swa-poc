{
  "navigationFallback": { 
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/api/*", "/partials/*", "/favicon.ico"] 
  },
  "routes": [
    { "route": "/aboutus",   "rewrite": "/aboutus.html" },
    { "route": "/contactus", "rewrite": "/contactus.html" },
    { "route": "/students",  "rewrite": "/students.html" },
    { "route": "/api/*", "allowedRoles": ["anonymous","authenticated"] }
  ],
  "responseOverrides": { 
    "404": { "rewrite": "/index.html" } 
  }
}
